{
  "expo": {
    "name": "HyGrow",
    "slug": "HyGrow-App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/hygrow.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/hygrow.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/hygrow.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ceg.HyGrowApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f309768e-8f5d-4183-965e-808bff964be4"
      }
    }
  }
}
